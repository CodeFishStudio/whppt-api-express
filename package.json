{
  "name": "@whppt/api-express",
  "description": "Whppt api module for express",
  "version": "2.0.0-rc.42",
  "author": "Svelte Studios (https://www.npmjs.com/settings/svelte-studios/packages)",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "jest",
    "format": "prettier -c src",
    "lint": "eslint --ext ts --ext js src/ --ignore-path .gitignore",
    "lint:fix": "yarn lint -- --fix",
    "watch": "run-p watch:*",
    "watch:js": "rm -rf dist && rm -rf types && tsc --watch",
    "build": "run-p build:*",
    "build:js": "rm -rf dist && rm -rf types && tsc",
    "publish:patch-rc-dry": "yarn test && yarn build && standard-version --release-as patch --prerelease rc --dry-run",
    "publish:patch-rc": "yarn test && yarn build && standard-version --release-as patch --prerelease rc && yarn publish --tag rc && git push --follow-tags",
    "publish:patch": "yarn test && yarn build && standard-version --release-as patch && yarn publish && git push --follow-tags",
    "publish:minor": "yarn test && yarn build && standard-version --release-as minor && yarn publish && git push --follow-tags",
    "publish:major": "yarn test && yarn build && standard-version --release-as major && yarn publish && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svelte-studios/whppt-api-express.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svelte-studios/whppt-api-express/issues"
  },
  "files": [
    "dist",
    "cli"
  ],
  "bin": {
    "gen-root-user": "cli/generateRootUser.js"
  },
  "homepage": "https://github.com/svelte-studios/whppt-api-express",
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-stage-2": "^7.8.3",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-env": "^22.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^5.5.3",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.2.13",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rembrandt": "^0.1.3",
    "sinon": "^9.2.1"
  },
  "dependencies": {
    "@types/jest": "^26.0.15",
    "aws-sdk": "^2.616.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.5",
    "email-syntax": "^2.0.2",
    "encoding": "^0.1.12",
    "express": "^4.17.1",
    "express-cache-headers": "^0.1.4",
    "express-promise-json-router": "^1.0.8",
    "file-type": "^14.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mongodb": "^3.4.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.7",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prompts": "^2.4.0",
    "require-glob": "^3.2.0",
    "sharp": "^0.25.1",
    "slugify": "^1.4.0",
    "standard-version": "^9.5.0",
    "uniqid": "^5.2.0",
    "uri-js": "^4.2.2"
  }
}
